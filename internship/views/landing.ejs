/* footer */
/* Container styling */
.how-it-works-container {
    padding: 40px 20px;
    max-width: 1200px;
    margin: 0 auto;
    text-align: center;
}

.title {
    font-size: 36px;
    font-weight: bold;
    margin-bottom: 10px;
    color: #333;
}

.description {
    font-size: 18px;
    color: #666;
    margin-bottom: 30px;
}

/* Steps container */
.steps-container {
    display: flex;
    justify-content: center;
    gap: 40px;
    flex-wrap: wrap;
    margin-top: 40px;
}

/* Individual step styling */
.step {
    width: 280px;
    text-align: center;
    margin-bottom: 30px;
}

.step-icon {
    width: 48px;
    height: 48px;
    margin-bottom: 16px;
}

.step-title {
    font-size: 22px;
    font-weight: bold;
    margin-bottom: 10px;
    color: #333;
}

.step-description {
    font-size: 16px;
    color: #555;
}

/* Call to Action button styling */
.cta-button {
    background-color: #007BFF;
    color: white;
    padding: 12px 30px;
    font-size: 18px;
    font-weight: bold;
    text-decoration: none;
    border-radius: 30px;
    margin-top: 40px;
    display: inline-block;
    transition: background-color 0.3s ease;
}

.cta-button:hover {
    background-color: #0056b3;
}
.step-icon {
    font-size: 48px; /* Adjust icon size */
    color: #007BFF; /* Icon color */
